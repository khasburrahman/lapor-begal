<!DOCTYPE html>
<html lang="en">

<%- include('./components/header', {title: "Register User"}) %>

<body>
  <%- include('./components/navbar') %>
  <%- include('./components/error') %>
  <%- include('./components/message') %>
  <div class="konteiner">
    <div class="cardHeader">
      <h5 id="headerTerdekat">Terdekat ></h5>
      <form action="/reports/addReport" method="GET">
        <button type="submit" class="addLaporan btn btn-danger">Tambah Laporan</button></form>
    </div>
    <div id="terdekat">
      <%for (const report of results) {%>
        <div class="card customCard reportCard">
          <div class="fotoLokasi">
          </div>
          <div class="card-body">
            <h5 class="card-title">Lokasi: <%= report.Location.nama %></h5>
            <p class="card-text card-text">Deskripsi: <%= report.deskripsi%></p>
          </div>
        </div>
        <%}%>

    </div>
  </div>
</body>

</html>
